
<%# @param model: modelのインスタンス %>

<div class="modal-dialog">
  <div class="modal-content">
    <div class="modal-body">
      
      <% if model.errors.any? %>
        <ul>
          <% model.errors.full_messages.each do |message| %>
            <li><%= message %></li>
          <% end %>
        </ul>
      <% end %>
      
    </div>
  </div>
</div>
